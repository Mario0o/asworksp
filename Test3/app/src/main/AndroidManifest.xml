<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yyh.test3" >

    <application android:icon="@drawable/ic_launcher" android:label="@string/app_name">
        <activity android:name="com.example.yyh.test3.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.yyh.test3.ServerActivity"></activity>

        <activity android:name="com.example.yyh.test3.ClientActivity"></activity>

        <service android:name="com.example.yyh.test3.com.example.yyh.utils.BluetoothServerService"></service>
        <service android:name="com.example.yyh.test3.com.example.yyh.utils.BluetoothClientService"></service>
    </application>
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />

</manifest>
